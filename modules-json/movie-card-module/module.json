      "settings": {
        "innerContent": {
          "groupType": "group-item",
          "item": {
            "groupName": "mainContent",
            "priority": 30,
            "render": true,
            "attrName": "synopsis.innerContent",
            "label": "Synopsis (From API)",
            "description": "Movie synopsis loaded from TMDB.",
            "features": {
              "sticky": false
            },
            "component": {
              "name": "divi/textarea",
              "type": "field",
              "props": {
                "disabled": true
              }
            }
          }
        },
        "decoration": {
          "font": {
            "priority": 10,
            "component": {
              "props": {
                "groupLabel": "Synopsis Text",
                "fieldLabel": "Synopsis"
